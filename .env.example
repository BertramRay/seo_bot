# 服务器配置
PORT=3000
NODE_ENV=development

# 数据库配置
MONGODB_URI=mongodb://localhost:27017/seo_bot

# OpenAI API配置
OPENAI_API_KEY=your_openai_api_key
OPENAI_MODEL=gpt-4o

# 博客配置
BLOG_TITLE=我的SEO博客
BLOG_DESCRIPTION=自动生成的高质量SEO博客内容
SITE_URL=https://yourdomain.com
BLOG_PATH=/blog

# 内容生成配置
CONTENT_GENERATION_FREQUENCY=daily  # daily, weekly, custom
POSTS_PER_BATCH=5
MIN_WORDS_PER_POST=800
MAX_WORDS_PER_POST=1500
CONTENT_AUTO_PUBLISH=true
CUSTOM_CRON_EXPRESSION=0 3 * * *

# SEO配置
DEFAULT_KEYWORDS=关键词1,关键词2,关键词3
LANGUAGE=zh-CN
GENERATE_SITEMAP=true
SITEMAP_UPDATE_FREQUENCY=daily

# 日志配置
LOG_LEVEL=info

# 用户认证配置
GITHUB_CLIENT_ID=your_github_client_id
GITHUB_CLIENT_SECRET=your_github_client_secret
GITHUB_CALLBACK_URL=http://localhost:3000/auth/github/callback
SESSION_SECRET=your_session_secret_key

# 管理员配置
ADMIN_EMAIL=admin@example.com 